{
    "name": "centamiv/req-res-log",
    "version": "0.2",
    "description": "Simple Laravel HTTP request/response logger",
    "license": "MIT",
    "keywords": ["request", "response", "logger", "laravel"],
    "authors": [
        {
            "name": "Ivan Centamori",
            "email": "centamori@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/log": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "ReqResLog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ReqResLog\\ReqResLogServiceProvider"
            ]
        }
    }
}
